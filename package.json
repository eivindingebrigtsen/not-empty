{
	"name": "not-empty-ts",
	"version": "1.0.1",
	"author": "uǝsʇƃᴉɹqǝƃuI puᴉʌᴉƎ",
	"scripts": {
		"build": "rm -rf dist && tsc",
		"test": "vitest",
		"test:ci": "vitest --run --coverage --reporter=verbose",
		"prepare": "npm run build",
		"lint": "biome check .",
		"format": "biome format . --write",
		"prepublishOnly": "npm test && npm run build",
		"preversion": "npm run lint",
		"version": "npm run format && git add -A src",
		"postversion": "git push && git push --tags",
		"release:patch": "npm version patch -m 'chore: release %s'",
		"release:minor": "npm version minor -m 'chore: release %s'",
		"release:major": "npm version major -m 'chore: release %s'"
	},
	"keywords": [
		"typescript",
		"javascript",
		"not-empty",
		"not-null",
		"not-undefined"
	],
	"license": "MIT",
	"type": "module",
	"types": "dist/index.d.ts",
	"exports": "./dist/index.js",
	"description": "A lightweight utility to check if an object is not empty, not null, or not undefined.",
	"files": ["dist", "README.md"],
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@vitest/coverage-v8": "^3.0.7",
		"typescript": "5.8.2",
		"vitest": "3.0.7"
	},
	"repository": "eivindingebrigtsen/not-empty",
	"bugs": {
		"url": "https://github.com/eivindingebrigtsen/not-empty/issues"
	},
	"homepage": "https://github.com/eivindingebrigtsen/not-empty#readme",
	"engines": {
		"node": ">=14.0.0"
	},
	"vitest": {
		"coverage": {
			"reporter": ["text", "json", "html"],
			"branches": 100,
			"functions": 100,
			"lines": 100,
			"statements": 100
		}
	}
}
